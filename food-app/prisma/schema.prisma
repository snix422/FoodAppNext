

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post {
  id        Int     @id @default(autoincrement())
  title     String
  content   String?
  author    String
}

model Kontakt {
  id      Int    @id @default(autoincrement())
  email   String
  telefon String
  adres   String
}

model AboutUs {
  id Int @id @default(autoincrement())
  content String
}

model Offer {
  id Int @id @default(autoincrement())
  title String
  description String
}
